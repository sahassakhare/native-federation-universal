{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "native-federation": {
      "projectType": "library",
      "root": "projects/native-federation",
      "sourceRoot": "projects/native-federation/src",
      "prefix": "nf",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/native-federation/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/native-federation/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/native-federation/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/native-federation/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/native-federation/**/*.ts",
              "projects/native-federation/**/*.html"
            ]
          }
        }
      }
    },
    "native-federation-schematics": {
      "projectType": "library",
      "root": "projects/native-federation-schematics",
      "sourceRoot": "projects/native-federation-schematics/src",
      "architect": {
        "build": {
          "builder": "@angular-devkit/architect:concat",
          "options": {
            "targets": [
              { "target": "native-federation-schematics:build-ts" },
              { "target": "native-federation-schematics:copy-assets" }
            ]
          }
        },
        "build-ts": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "projects/native-federation-schematics/tsconfig.json",
            "outputPath": "dist/native-federation-schematics"
          }
        },
        "copy-assets": {
          "builder": "@angular-devkit/architect:copy",
          "options": {
            "assets": [
              {
                "input": "projects/native-federation-schematics",
                "glob": "collection.json",
                "output": "."
              },
              {
                "input": "projects/native-federation-schematics/src",
                "glob": "**/*.json",
                "output": "src"
              },
              {
                "input": "projects/native-federation-schematics",
                "glob": "package.json",
                "output": "."
              }
            ],
            "outputPath": "dist/native-federation-schematics"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/native-federation-schematics/**/*.ts"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}